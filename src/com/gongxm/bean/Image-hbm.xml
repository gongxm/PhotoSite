<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.gongxm.bean">
	<class name="Image" table="images">
		<id name="id" column="ID">
			<generator class="native"></generator>
		</id>
		<property name="filename" column="FILENAME"></property>
		<property name="filepath" column="FILEPATH"></property>
		<property name="imagesPath" column="IMAGESPATH"></property>
		<property name="category" column="CATEGORY"></property>
	</class>
</hibernate-mapping>